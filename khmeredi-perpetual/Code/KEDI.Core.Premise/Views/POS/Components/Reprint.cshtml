
<div id='reprint-content' class="widget-scrollbar" style="display: none;">
    <div class="group-search-boxes">
        <div id="text-search" class="search-box show">
            <div class="input-box">
                <i id="i-search" class="i-search fas fa-search"></i>
                <input class="ignore" id="search-reprint-receipts" type="text" value="" placeholder=@Localizer["Search ..."]>
                <i id="i-close" class="i-close far fa-times-circle"></i>
            </div>
            <div id="btn-search" style="display: none;" class="btn-search no-space"><i class="fas fa-search"></i></div>
        </div>
    </div>
    <div class="flexbox">
        <div class="stack-block width-half">
            <label>@Localizer["Date From"]</label>
            <input type="date" class="reprint-date-from" />
        </div>
        <div class="stack-block width-half">
            <label>@Localizer["Date To"]</label>
            <input type="date" class="reprint-date-to" />
        </div>
    </div>
   
    <fieldset id="reprint-receipt" class='shadow wrap-table widget-scrollbar' style="height: 350px;">
        <legend class="title">@Localizer["List of Receipts"]</legend>
        <table class="reprint-receipt-listview">
            <tr>
                <th>@Localizer["Receipt №"]</th>
                <th>@Localizer["Cashier"]</th>
                 <th>@Localizer["Name"]</th>
                <th>@Localizer["Phone Number"]</th> 
                <th>@Localizer["Date"]</th>
                <th>@Localizer["Time"]</th>
                <th>@Localizer["Table"]</th>
                <th>@Localizer["Amount"]</th>
                <th></th>
            </tr>

        </table>
    </fieldset>
</div>