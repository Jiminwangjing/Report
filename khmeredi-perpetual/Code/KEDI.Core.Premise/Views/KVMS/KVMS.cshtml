                    <td class="member-id"></td>
                </tr>
                <tr>
                    <td>@Localizer["Name"]</td>
                    <td>:</td>
                    <td class="member-name"></td>
                </tr>
                <tr>
                    <td>@Localizer["Card Type"]</td>
                    <td>:</td>
                    <td class="member-card-type"></td>
                </tr>
                <tr>
                    <td>@Localizer["Discount"]</td>
                    <td>:</td>
                    <td class="member-discount"></td>
                </tr>
                <tr>
                    <td>@Localizer["Expire Date"]</td>
                    <td>:</td>
                    <td class="member-expire"></td>
                </tr>
                <tr>
                    <td>@Localizer["Description"]</td>
                    <td>:</td>
                    <td class="member-descr"></td>
                </tr>

            </table>

            <hr />
        </div>

    </div>
</div>

<!-----------------------------Discount Member Card--------------------------->
<div id="item-add-on" style="display: none;">
    <div class='wrap-block col-two'>
        <div class='left-block m0'>
            <div class="block">
                <nav class='nav-toolbar'>
                    <div class='item'>
                        <div class='btn'
                             style="height: 30px; line-height: 30px;">
                            <i class="far fa-plus-square"></i>
                        </div>
                    </div>
                    <div class="item group-search-boxes">
                        <div id="text-search" class="search-box show">
                            <div class="input-box" style="max-width: 326px;">
                                <i id="i-search" class="i-search fas fa-search"></i>
                                <input class="ignore" name="search-member" type="text" value="" placeholder=@Localizer["Code,Name search ..."]>
                                <i id="i-close" class="i-close far fa-times-circle"></i>
                            </div>
                            <div id="btn-search" style="display: none;" class="btn-search no-space"><i class="fas fa-search"></i></div>
                        </div>
                    </div>
                </nav>
                <div class='wrap-table list scroll-xy bg-gray'
                     style="height: 350px; margin: .5rem 0 0 .25rem;">
                    <table class="item-add-on-listview">
                        <tr>
