@using KEDI.Core.Premise.Models.Services.ServiceContractTemplate;
<style>
    .cursor {
        cursor: pointer;
    }

    .font-size {
        font-size: 13px;
    }

    .loading {
        width: 100vw;
        height: 90%;
        z-index: 9999999999999999999999999;
        position: absolute;
    }

    .img-loading {
        position: absolute;
        top: 79%;
        left: 38%;
    }

    .bTable {
        overflow: auto;
        height: 177px;
    }

    .loadingitem {
        position: absolute;
        top: 46%;
        left: 46%;
    }

    .input-group .input-group-text,
    .input-group select,
    .input-group input {
        min-width: 38px !important;
    }

    .disabled {
        pointer-events: none;
    }

    table thead tr th,
    table tr th,
    table tbody tr,
    table tr td {
        text-align: left;
    }

    i.fa-paperclip {
        position: relative;
        z-index: 10;
    }

    i.fa-paperclip form {
        width: 10px !important;
        height: 15px !important;
        opacity: 0;
        overflow: hidden;
        position: absolute;
        margin-top: -15px;
    }

    i.fa-paperclip form .box_file input[type=file] {
        margin-top: -5px;
        display: block;
        opacity: 0;
        position: absolute;
        z-index: 100;
    }

    .tab-custome {
        background-color: #F4FCFF !important;
        margin-left: -2.3% !important;
        width: 104.6% !important;
    }
</style>
