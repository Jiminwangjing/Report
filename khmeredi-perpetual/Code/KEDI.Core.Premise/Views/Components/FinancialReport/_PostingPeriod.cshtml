<div id="postingPeriod-content" style="display:none; height: 90vh">
    <div class="item group-search-boxes">
        <div id="text-search" class="wrap-search-box">
            <div class="wrap-input show">
                <i id="i-search" class="i-search fas fa-search"></i>
                <input class="ignore font-size" id="find-cus" autocomplete="off" type="text" placeholder="Search ...">
                <i id="i-close" class="i-close far fa-times-circle"></i>
            </div>
            <div id="btn-search" class="btn-search" style="display: none;"><i class="fas fa-search"></i></div>
        </div>
    </div>
    <div id="container-list-posting-period" class="wrap-table  widget-scrollbar" style="height: auto">
        <table class="posting-period">
            <thead>
                <tr>
                    <th class="text-center font-size">@Localizer["#"]</th>
                    <th class="text-center font-size">@Localizer["Period Code"]</th>
                    <th class="text-center font-size">@Localizer["Period Name"]</th>
                    <th class="text-center font-size">@Localizer["Choose"]</th>
                </tr>
            </thead>
        </table>
    </div>
</div>