        </tr>
    </table>
</div>
@*//model box of doctypeno*@
<div class="doctypeno_containers widget-scrollbar" style="display: none; overflow: auto">
    <div class="group-search-boxes mb-1" style="display:flex;justify-content:flex-end ">
        <div id="text-search" class="wrap-search-box ">
            <div class="wrap-input show">
                <i id="i-search" class="i-search fas fa-search"></i>
                <input class="ignore" id="search-doctype" autocomplete="off" type="text"
                    placeholder="@Localizer["Search ..."]">
                <i id="i-close" class="i-close far fa-times-circle"></i>
            </div>
            <div id="btn-search" class="btn-search" style="display: none;"><i class="fas fa-search"></i></div>
        </div>
    </div>
    <table id="list-doctypeno">
        <tr>
            <th>@Localizer["#"]</th>
            <th>@Localizer["Date"]</th>
            <th>@Localizer["Dudate"]</th>
            <th>@Localizer["Customer"]</th>
            <th>@Localizer["Remark"]</th>
            <th>@Localizer["Choose"]</th>
        </tr>
    </table>
</div>
@*//model box of interest level*@
<div id="active-level-content" style="display:none; height: 250px; ">
    <div class="group-search-boxes mb-1" style="display:flex;justify-content:flex-end ">
        <div id="text-search" class="wrap-search-box ">
            <div class="wrap-input show">
                <i id="i-search" class="i-search fas fa-search"></i>
                <input class="ignore" id="search-levelinterest" autocomplete="off" type="text"
                    placeholder="@Localizer["Search ..."]">
                <i id="i-close" class="i-close far fa-times-circle"></i>
            </div>
            <div id="btn-search" class="btn-search" style="display: none;"><i class="fas fa-search"></i></div>
        </div>
    </div>
    <div>
        <table id="level-interest">
            <thead>
                <tr>
                    <th><i id="create-level-description" class="fas fa-plus-square" style="color:sandybrown;"></i></th>
                    <th style="width:70px">Description</th>
                </tr>
            </thead>

        </table>
    </div>
    <div style="display:flex;justify-content:flex-end">
        <p id="sms_level"></p>
    </div>

</div>
@*//model box of set up interest range*@
<div id="active-setuprange-content" style="display:none; height: 250px; ">
    <div class="group-search-boxes mb-1" style="display:flex;justify-content:flex-end ">
        <div id="text-search" class="wrap-search-box ">
            <div class="wrap-input show">
                <i id="i-search" class="i-search fas fa-search"></i>
                <input class="ignore" id="search-setuprange" autocomplete="off" type="text"
                    placeholder="@Localizer["Search ..."]">
